<script>
function myTrim(x) {
    return x.replace(/^\s+|\s+$/gm,'');
}

function search_json() {
    var kw = myTrim($("#kw").val());
    if(!kw) {
        return;
    }

    location.href = "/book/search/result/31?keyword="+kw;
}

$(document).ready(function () {
    $(".book-search").on("click", function() {
        $("#search_form").submit();
    });

    $("#mobile_search_btn").on("click", function() {
        search_json();
    });

    $(".home").on("click", function() {
        location.href = "/";
    });

    if (typeof(Storage) !== "undefined") {
        if(localStorage.sidebar == "hidden") {
            menuToggle();
        }else {
            $(".sidebar").show();
            $(".toc-header").show();
        }
    }else {
        $(".sidebar").show();
        $(".toc-header").show();
    }

    $(".toc").css("display", "block");


    if (typeof(Storage) !== "undefined") {
        // console.log('sp:' + localStorage.sp)
        $(".sidebar").scrollTop(localStorage.sp);
    }

    var $activeItem = $(".list-group-item.active");
    if ($activeItem.length) {
        var sidebar = $(".sidebar");
        var sidebarTop = sidebar.offset().top;
        var sidebarBottom = sidebarTop + sidebar.height();
        var itemTop = $activeItem.offset().top;
        var itemBottom = itemTop + $activeItem.outerHeight();

        // 선택된 메뉴 항목이 현재 보이는 영역 내에 있는지 확인
        if (itemTop < sidebarTop || itemBottom > sidebarBottom) {
            var scrollPosition = sidebar.scrollTop() + (itemTop - sidebarTop);
            sidebar.scrollTop(scrollPosition);
        }
    }

    $(".sidebar").css("overflow-y", "auto");

    $(".toc_item").on("click", function() {
        var page_id = $(this).attr("page_id");
        // $(".selected_toc").removeClass("selected_toc");
        // $(this).addClass("selected_toc");
        page(page_id);

    });

    $(".menu-toggle").on("click", function() {
        menuToggle();

        if (typeof(Storage) !== "undefined") {
            if(localStorage.sidebar == "hidden") {
                localStorage.sidebar = "show";
            }else {
                localStorage.sidebar = "hidden";
            }
        }
    });

    
    $("input[name=toc_on_off]").on("click", function() {
        var result = $(this).val();
        if (typeof(Storage) !== "undefined") {
            localStorage.toc = result;
        }
    });
    if (typeof(Storage) !== "undefined") {
        if (localStorage.getItem("toc") === null) {  // init value
            localStorage.toc = "hide";
        }
        if(localStorage.toc == "hide") {
            $('#toc_off').prop('checked', true);
            $(".toc-checker").hide();
        }else {
            $('#toc_on').prop('checked', true);
            $(".toc-checker").removeClass("hide");
            $(".toc-checker").show();
        }
    }
    

    $("#load_content").show();
});


function menuToggle(no_sidebar) {
    if(!no_sidebar) {
        $(".sidebar").toggle();
        $(".toc-header").toggle();
    }
    $("#load_content").toggleClass("col-sm-offset-3");
    $("#load_content").toggleClass("col-sm-9");
    $("#load_content").toggleClass("col-sm-12");
    $("#load_content").toggleClass("sidebar-padding");
    $(".prev_next_indicator").toggle();
    $(".menu-group").toggle();
}

function page(no) {
    var scrollpos = $(".sidebar").scrollTop();
    if (typeof(Storage) !== "undefined") {
        localStorage.sp = scrollpos;
    }

    location.href = "/"+no;
    return false;

    $(".selected_toc").removeClass("selected_toc");
    $(".toc_item").each(function() {
        var page_id = $(this).attr("page_id");
        if(page_id == no) {
            $(this).addClass("selected_toc");
        }
    });

    var url = "/load/"+no;
    $("#load_content").load(url, function(responseTxt, statusTxt, xhr){
        if(statusTxt == "success") {
            // alert("External content loaded successfully!");
            $("#load_content").scrollTop(0);
            window.history.pushState("object or string", "01장 자바란 무엇인가?", "/"+no);
        }
        if(statusTxt == "error") {
            alert("Error: " + xhr.status + ": " + xhr.statusText);
        }
    });

